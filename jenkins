pipeline {
    agent any
    stages {
        stage("Docker Version") {
            steps {
                sh "docker --version"
            }
        }
        stage("git Version") {
            steps {
                sh "git --version"
            }
        }
        
        stage('Build Docker Image') {
            steps {
                sh 'docker build -t myimage .'
            }
        }
     
    }
   
}
